{%
  set classes = [
    'footer',
    'container',
    'region',
    'region-' ~ region|clean_class,
  ]
%}
{% if content %}
  <div{{ attributes.addClass(classes) }}>
    {{ content }}
    <div class="footer__privacy">
      <div>&#169; 2010 &ndash; {{ post.published_at|date("Y") }}</div>
      <div><a target="_blank" href="/#">{{ 'Privacy &ndash; Terms'|t }}</a></div>
    </div>
  </div>
{% endif %}
